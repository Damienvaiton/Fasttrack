<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardSchedule"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardElevation="0dp"
    android:backgroundTint="@color/clear"
    app:cardUseCompatPadding="false">

    <ImageView
        android:id="@+id/divSchedulePrincipal"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_marginHorizontal="5dp"
        android:src="@drawable/rectqatarschedule"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/divScheduleGagnant"
        android:layout_width="100dp"
        android:layout_height="60dp"
        android:src="@drawable/rectgagnantqatarschedule"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/divSchedulePrincipal"
        app:layout_constraintHorizontal_bias="0.913"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.6"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/divScheduleLarge"
        android:layout_width="116dp"
        android:layout_height="75dp"
        android:src="@drawable/scheduleqatarrectlarge"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/divSchedulePrincipal"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="@+id/divSchedulePrincipal"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.36"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/scheduleTrack"
        android:layout_width="100dp"
        android:layout_height="70dp"
        android:src="@drawable/qatartrack"
        app:layout_constraintBottom_toBottomOf="@+id/divScheduleLarge"
        app:layout_constraintEnd_toEndOf="@+id/divScheduleLarge"
        app:layout_constraintStart_toStartOf="@+id/divScheduleLarge"
        app:layout_constraintTop_toTopOf="@+id/divScheduleLarge"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/nameGpSchedule"
        android:layout_width="300px"
        android:layout_height="wrap_content"
        android:fontFamily="@font/leckerli_one"
        android:maxWidth="300px"
        android:text="FastTrack"
        android:textColor="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.528"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/divSchedulePrincipal"
        app:layout_constraintVertical_bias="0.134"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>